<!--index.wxml-->



<view class="cu-bar search" >
 <view class='search-form round'>
    <text class="icon-search"></text>
    <input type="text" placeholder="搜索试卷、知识点、题目"  bindinput='search' confirm-type="search"></input>
  </view>
   <view class='action'>
    <button class='cu-btn bg-orange shadow-blur round' bindtap='goworld'>搜索</button>
  </view>
 </view>




<view class='whole' >


<import src="/welCropper/welCropper.wxml" />
<template is="welCropper" data="{{cropperData:cropperData, cropperMovableItems:cropperMovableItems, cropperChangableData:cropperChangableData}}"></template>

<view class='padding  text-center' style='margin-top:100rpx; '>
  <image src='../image/search.png' bindtap='selectTap' data-mode='rectangle' mode='widthFix' style='width:200rpx;'></image>
</view>
<view class='padding text-center text-white lg' style='margin-top:80rpx;font-weight:800;'> 拍照试试 </view>




</view> 